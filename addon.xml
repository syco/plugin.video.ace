    <assets>
      <icon>icon.png</icon>
    </assets>
    <forum>https://github.com/syco/plugin.video.ace</forum>
    <website>https://github.com/syco/plugin.video.ace</website>
    <email></email>
    <source>https://github.com/syco/plugin.video.ace</source>
